            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>consBanco</from-outcome>
            <to-view-id>/pages/banco/consbanco.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
     <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>consProduto</from-outcome>
            <to-view-id>/pages/produtos/consProduto.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>consTipoPlanoConta</from-outcome>
