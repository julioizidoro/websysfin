            <from-outcome>relContasPagar</from-outcome>
            <to-view-id>/pages/contaspagar/relContasPagar.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>selecionarUnidade</from-outcome>
            <to-view-id>/pages/cliente/selecionarUnidade.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>consOutrosLancamentos</from-outcome>
            <to-view-id>/pages/lancamentos/consOutrosLancamentos.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>consPlanoConta</from-outcome>
